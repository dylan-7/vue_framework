{
  "name": "new_admin",
  "version": "0.0.1",
  "description": "admin",
  "author": "dylan",
  "scripts": {
    "serve": "cross-env NODE_ENV=development webpack-dev-server --config config/webpack.config.js --mode development --progress",
    "build": "cross-env NODE_ENV=production webpack --config config/webpack.config.js --mode production --progress",
    "build:serve": "serve public --single",
    "lint": "eslint --ext .js,.ts,.vue src",
    "analyz": "analyzerReport=true yarn build"
  },
  "dependencies": {
    "axios": "^0.19.0",
    "element-ui": "^2.12.0",
    "normalize.css": "^8.0.1",
    "vue": "^2.6.10",
    "vue-class-component": "^7.1.0",
    "vue-i18n": "^8.15.0",
    "vue-property-decorator": "^8.3.0",
    "vue-router": "^3.1.3",
    "vuex": "^3.1.1",
    "vuex-class": "^0.3.2"
  },
  "devDependencies": {
    "@types/axios": "^0.14.0",
    "@vue/cli-plugin-eslint": "^4.0.5",
    "@vue/eslint-config-standard": "^4.0.0",
    "@vue/eslint-config-typescript": "^4.0.0",
    "clean-webpack-plugin": "^3.0.0",
    "cross-env": "^6.0.3",
    "css-loader": "^3.2.0",
    "eslint": "^6.6.0",
    "eslint-friendly-formatter": "^4.0.1",
    "eslint-loader": "^3.0.2",
    "eslint-plugin-node": "^10.0.0",
    "eslint-plugin-vue": "^6.0.0",
    "file-loader": "^4.2.0",
    "fork-ts-checker-webpack-plugin": "^3.0.1",
    "html-webpack-plugin": "^3.2.0",
    "husky": "^3.0.9",
    "lint-staged": "^9.4.2",
    "node-sass": "^4.13.0",
    "optimize-css-assets-webpack-plugin": "^5.0.3",
    "postcss-loader": "^3.0.0",
    "postcss-safe-parser": "^4.0.1",
    "prettier": "^1.18.2",
    "sass-loader": "^8.0.0",
    "serve": "^11.2.0",
    "style-loader": "^1.0.0",
    "ts-loader": "^6.2.1",
    "typescript": "^3.7.2",
    "url-loader": "^2.2.0",
    "vue-loader": "^15.7.2",
    "vue-style-loader": "^4.1.2",
    "vue-template-compiler": "^2.6.10",
    "webpack": "^4.41.2",
    "webpack-bundle-analyzer": "^3.6.0",
    "webpack-cli": "^3.3.10",
    "webpack-dev-server": "^3.9.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.{ts,js,vue}": [
      "prettier --single-quote --trailing-comma es5 --end-of-line lf --write",
      "git add"
    ],
    "yarn.lock": [
      "git rm --cached"
    ]
  },
  "browserslist": [
    "> 1%",
    "last 2 versions"
  ]
}
